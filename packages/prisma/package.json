{
  "name": "@package/prisma",
  "version": "0.0.0",
  "files": [
    "lib"
  ],
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "run prebuild && run -T rollup -c && run postbuild",
    "package": "run build",
    "postbuild": "run -T tsc --project tsconfig.build.json --emitDeclarationOnly",
    "prebuild": "run -T rimraf ./lib",
    "test": "run -T jest --watch",
    "test-ci": "run -T jest --coverage"
  },
  "dependencies": {
    "@nestjs/common": "^8.1.1",
    "@prisma/client": "^3.3.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.4.0"
  }
}
